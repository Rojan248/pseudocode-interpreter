DECLARE Matrix : ARRAY[1:3, 1:3] OF INTEGER
DECLARE i : INTEGER
DECLARE j : INTEGER

FOR i <- 1 TO 3
    FOR j <- 1 TO 3
        Matrix[i, j] <- i * j
    NEXT j
NEXT i

OUTPUT "Matrix 3x3:"
FOR i <- 1 TO 3
    FOR j <- 1 TO 3
        OUTPUT "Matrix[", i, ",", j, "] =", Matrix[i, j]
    NEXT j
NEXT i
